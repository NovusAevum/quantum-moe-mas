# Quantum MoE MAS Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# =============================================================================
# ENVIRONMENT CONFIGURATION
# =============================================================================
ENVIRONMENT=development  # development, staging, production
DEBUG=true
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# PostgreSQL Configuration (if using direct connection)
DATABASE_URL=postgresql://username:password@localhost:5432/quantum_moe_mas
DB_HOST=localhost
DB_PORT=5432
DB_NAME=quantum_moe_mas
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_SSL_MODE=prefer

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password

# =============================================================================
# AI/ML API KEYS
# =============================================================================
# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORG_ID=your_openai_org_id_here

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Hugging Face Configuration
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Google AI Studio
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Groq Configuration
GROQ_API_KEY=your_groq_api_key_here

# Cerebras Configuration
CEREBRAS_API_KEY=your_cerebras_api_key_here

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Cohere Configuration
COHERE_API_KEY=your_cohere_api_key_here

# Replicate Configuration
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Stability AI Configuration
STABILITY_API_KEY=your_stability_api_key_here

# =============================================================================
# QUANTUM COMPUTING
# =============================================================================
# IBM Quantum Configuration
IBM_QUANTUM_TOKEN=your_ibm_quantum_token_here
IBM_QUANTUM_INSTANCE=ibm_quantum
IBM_QUANTUM_BACKEND=simulator_statevector

# =============================================================================
# CLOUD SERVICES
# =============================================================================
# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_DEFAULT_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket_name

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT_ID=your_gcp_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/gcp/credentials.json

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id
AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret
AZURE_TENANT_ID=your_azure_tenant_id

# =============================================================================
# MARKETING & CRM
# =============================================================================
# HubSpot Configuration
HUBSPOT_API_KEY=your_hubspot_api_key_here
HUBSPOT_CLIENT_ID=your_hubspot_client_id
HUBSPOT_CLIENT_SECRET=your_hubspot_client_secret

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
# JWT Configuration
JWT_SECRET_KEY=your_super_secret_jwt_key_here_min_32_chars
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Encryption Keys
ENCRYPTION_KEY=your_32_byte_encryption_key_here
FERNET_KEY=your_fernet_key_here

# API Security
API_RATE_LIMIT_PER_MINUTE=100
API_RATE_LIMIT_PER_HOUR=1000

# =============================================================================
# WEB APPLICATION
# =============================================================================
# Streamlit Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost
STREAMLIT_THEME_BASE=light

# FastAPI Configuration
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000
FASTAPI_RELOAD=true
FASTAPI_WORKERS=1

# =============================================================================
# MONITORING & OBSERVABILITY
# =============================================================================
# Sentry Configuration
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development

# Prometheus Configuration
PROMETHEUS_PORT=9090
METRICS_ENABLED=true

# New Relic Configuration
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key
NEW_RELIC_APP_NAME=quantum-moe-mas

# =============================================================================
# PERFORMANCE & CACHING
# =============================================================================
# Cache Configuration
CACHE_TTL_SECONDS=3600
CACHE_MAX_SIZE=1000
ENABLE_QUERY_CACHE=true

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# =============================================================================
# BUSINESS INTELLIGENCE
# =============================================================================
# ROI Tracking
ROI_TARGET_ICM_PER_SESSION=0.50
EFFICIENCY_GAIN_TARGET_PERCENT=25
COST_OPTIMIZATION_TARGET_PERCENT=40

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_RETENTION_DAYS=90

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================
# Testing Configuration
TEST_DATABASE_URL=postgresql://test_user:test_pass@localhost:5432/test_quantum_moe_mas
PYTEST_TIMEOUT=300

# Development Tools
ENABLE_DEBUG_TOOLBAR=true
ENABLE_PROFILING=false
MOCK_EXTERNAL_APIS=false

# =============================================================================
# DEPLOYMENT
# =============================================================================
# Container Configuration
CONTAINER_PORT=8000
HEALTH_CHECK_INTERVAL=30

# Kubernetes Configuration
K8S_NAMESPACE=quantum-moe-mas
K8S_SERVICE_ACCOUNT=quantum-moe-mas-sa

# Vercel Configuration (for serverless deployment)
VERCEL_URL=your_vercel_url_here
VERCEL_TOKEN=your_vercel_token_here